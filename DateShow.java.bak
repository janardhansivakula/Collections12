//DateShow.java
import java.util.*;
class DateShow
{
	public static void main(String args[])
	{
		Date dt=new Date();
		System.out.println("========================");
		System.out.println("The system current date is=" +dt);
		System.out.println("The System hours is="  +dt.getHours());
		System.out.println("The System hours is="  +dt.getMinutes());
		System.out.println("The System hours is="  +dt.getSeconds());
		System.out.println("========================");
	}
}